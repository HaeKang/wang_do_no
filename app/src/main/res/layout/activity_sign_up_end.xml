<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Sign.SignUpEnd">

    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline5" app:layout_constraintGuide_begin="30dp"
                                          android:orientation="vertical"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline6"
                                          android:orientation="vertical" app:layout_constraintGuide_end="31dp"/>
    <Button
            android:layout_width="146dp"
            android:layout_height="56dp"
            android:id="@+id/Main_Btn" app:layout_constraintStart_toStartOf="@+id/guideline5"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline6" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:fontFamily="@font/nanumsquareroundr" android:background="@drawable/login_btn"
            app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/text_user"/>
    <TextView
            android:text="가입이 완료되었습니다!"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_welcom" app:layout_constraintStart_toStartOf="@+id/guideline5"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline6" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:textSize="24sp"
            android:fontFamily="@font/nanumsquareroundr"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" android:layout_marginTop="32dp"/>
    <TextView
            android:layout_width="157dp"
            android:layout_height="47dp"
            android:id="@+id/text_user" android:textSize="18sp" app:layout_constraintStart_toStartOf="@+id/guideline5"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline6" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:fontFamily="@font/nanumsquareroundr"
            android:gravity="center"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/text_welcom"/>
    <ImageView
            android:layout_width="216dp"
            android:layout_height="195dp" app:srcCompat="@drawable/party"
            android:id="@+id/imageView6" android:layout_marginTop="68dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline5" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline6"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.495"
    />
</android.support.constraint.ConstraintLayout>