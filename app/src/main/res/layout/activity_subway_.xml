<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".subway.Subway_">


    <kr.go.seoul.trafficsubway.TrafficSubwayButtonTypeB
            android:layout_width="151dp"
            android:layout_height="158dp" android:id="@+id/type_b"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginTop="50dp" app:layout_constraintTop_toTopOf="parent">

    </kr.go.seoul.trafficsubway.TrafficSubwayButtonTypeB>

    <ImageButton
            android:layout_width="159dp"
            android:layout_height="138dp" app:srcCompat="@drawable/subway_find"
            android:id="@+id/Find_btn" android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/type_b" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:scaleType="fitCenter"
            android:background="#ffffff"/>

    <ImageButton
            android:layout_width="159dp"
            android:layout_height="138dp" app:srcCompat="@drawable/subway_friend"
            android:id="@+id/Freind_btn"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/Find_btn"
            android:scaleType="fitCenter"
            android:background="#ffffff"
    />


</android.support.constraint.ConstraintLayout>