<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Subway_">


    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="672dp" android:id="@+id/linearLayout" tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="55dp"/>

    <kr.go.seoul.trafficsubway.TrafficSubwayButtonTypeB
            android:layout_width="391dp"
            android:layout_height="603dp" android:id="@+id/type_b" tools:layout_editor_absoluteY="28dp"
            tools:layout_editor_absoluteX="16dp">

    </kr.go.seoul.trafficsubway.TrafficSubwayButtonTypeB>


    <android.support.design.widget.BottomNavigationView
            android:id="@+id/navigation"
            android:layout_width="0dp"
            android:layout_height="58dp"
            android:background="?android:attr/windowBackground"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:menu="@menu/navigation"/>

</android.support.constraint.ConstraintLayout>