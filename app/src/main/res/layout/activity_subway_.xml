<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".subway.Subway_">


    <kr.go.seoul.trafficsubway.TrafficSubwayButtonTypeB
            android:layout_width="151dp"
            android:layout_height="158dp" android:id="@+id/type_b"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginTop="50dp" app:layout_constraintTop_toTopOf="parent">

    </kr.go.seoul.trafficsubway.TrafficSubwayButtonTypeB>

    <ImageButton
            android:layout_width="159dp"
            android:layout_height="138dp" app:srcCompat="@drawable/subway_find"
            android:id="@+id/Find_btn"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:scaleType="fitCenter"
            android:background="@color/transparent" app:layout_constraintHorizontal_bias="0.516"
            android:layout_marginTop="276dp" app:layout_constraintTop_toTopOf="parent"/>

    <ImageButton
            android:layout_width="159dp"
            android:layout_height="138dp" app:srcCompat="@drawable/subway_friend"
            android:id="@+id/Freind_btn"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:scaleType="fitCenter"
            android:background="@color/transparent"
            app:layout_constraintHorizontal_bias="0.516" android:layout_marginTop="68dp"
            app:layout_constraintTop_toBottomOf="@+id/Find_btn"/>
    <ImageButton
            android:layout_width="30dp"
            android:layout_height="37dp" app:srcCompat="@drawable/ic_home_black_24dp"
            android:id="@+id/main_btn"
            android:background="@color/transparent" android:tint="@color/cardview_dark_background"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"/>


</android.support.constraint.ConstraintLayout>