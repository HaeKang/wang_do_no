<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Fragment.S_Find_Fragment">


    <Button
            android:text="중간찾기"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/middle_btn" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.583"
            android:layout_marginTop="40dp" app:layout_constraintTop_toBottomOf="@+id/end_text_mid"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="수원"
            android:ems="10"
            android:id="@+id/start_text_mid" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp" android:layout_marginTop="112dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="잠실"
            android:ems="10"
            android:id="@+id/end_text_mid" android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="60dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/start_text_mid"/>
    <TextView
            android:text="TextView"
            android:layout_width="169dp"
            android:layout_height="162dp" tools:layout_editor_absoluteY="400dp"
            android:id="@+id/test" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:gravity="center"/>

</android.support.constraint.ConstraintLayout>