<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".Fragment.Find_Fragment">

    <!-- TODO: Update blank fragment layout -->
    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <EditText
                android:layout_width="231dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/start_text"
                android:hint="출발지를 입력하세요" android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.725"
                android:layout_marginTop="120dp" app:layout_constraintTop_toTopOf="parent" android:text="수원"/>
        <EditText
                android:layout_width="231dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/end_text"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintHorizontal_bias="0.725" android:hint="목적지를 입력하세요" android:layout_marginTop="15dp"
                app:layout_constraintTop_toBottomOf="@+id/start_text" android:text="잠실"/>
        <Button
                android:text="길찾기"
                android:layout_width="wrap_content"
                android:layout_height="42dp"
                android:id="@+id/find_btn" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:layout_marginTop="24dp"
                app:layout_constraintTop_toBottomOf="@+id/end_text" app:layout_constraintHorizontal_bias="0.563"/>
        <TextView
                android:text="TextView"
                android:layout_width="213dp"
                android:layout_height="104dp"
                android:id="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:gravity="center"
                android:layout_marginTop="120dp" app:layout_constraintTop_toBottomOf="@+id/end_text"/>
    </android.support.constraint.ConstraintLayout>

</FrameLayout>